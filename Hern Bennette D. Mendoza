import java.util.Scanner;  // Import the Scanner class

class Main {
  public static void main(String[] args) {
    Scanner s = new Scanner(System.in);  // Create a Scanner object
    System.out.println("Enter your full name");//asking for name

    String fname = s.nextLine();  // Read user input
    System.out.println("Username is: " + fname);  // Output user input--Mendoza

    System.out.println("Enter your Age");//Asking for Age
	  int Age = s.nextInt();
	  System.out.println("Your Age is: " + Age); //Outputs Age--Punzalan
  }
}
